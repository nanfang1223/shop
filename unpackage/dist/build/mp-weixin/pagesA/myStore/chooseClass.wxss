.container{min-height:100vh;background-color:#f5f5f5}.container .jiantou{width:27rpx;height:15rpx;margin-left:auto;padding:8rpx 20rpx}.container .content{background-color:#fff;padding-left:30rpx}.container .content .list_top{border-bottom:1px solid #e5e5e5}.container .content .list_item{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-align:center;-webkit-align-items:center;align-items:center;padding:24rpx 0}.container .content .list_bottom{padding-left:60rpx;border-bottom:1px solid #e5e5e5}.container .content .list_bottom .list_item{padding:20rpx 0}.container .content .list .label{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-align:center;-webkit-align-items:center;align-items:center}.container .content .list .label text{font-size:26rpx;line-height:26rpx}.container .content .list .label .checkBox{width:30rpx;height:30rpx;position:relative;-webkit-box-sizing:border-box;box-sizing:border-box;margin-right:20rpx;background-color:#fff;border:1px solid #b7b7b7;-webkit-border-radius:4rpx;border-radius:4rpx;background-size:80%;background-position:50%;background-repeat:no-repeat}.container .content .list .label .checkBox.active{border-color:#010101}.container .bottomBox{height:98rpx}.container .bottomBox > view{background-color:#ff612a;color:#fff;border:0!important;position:fixed;bottom:0;left:0;right:0;height:98rpx;font-size:30rpx;line-height:98rpx;text-align:center}.pl_60{padding-left:60rpx!important}